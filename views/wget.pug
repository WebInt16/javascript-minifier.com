extends layout

block content
  h3.muted Wget
  pre.
    # minifying some JavaScript
    wget --post-data='input=console.log( 1 );' --output-document=- https://#{pkg.name}/raw

    # minifying a JS file (e.g. ready.js -> ready.min.js)
    wget --post-data="input=`cat ready.js`" --output-document=ready.min.js https://#{pkg.name}/raw

  include ./language-footer
